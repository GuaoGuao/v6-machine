<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="TestMachineDomain">
    <select id="getTestMachine" parameterType="map" resultType="map">
        SELECT
            *
        FROM
            ICA_ITEM_PARMS1
        WHERE
            1=1
            <if test="packBarSearch != null  and packBarSearch != '' ">
                AND PACK_BAR=#{packBarSearch}
            </if>
            <if test="distModeSearch != null  and distModeSearch != '' ">
                AND DIST_MODE=#{distModeSearch}
            </if>
    </select>
</mapper>
